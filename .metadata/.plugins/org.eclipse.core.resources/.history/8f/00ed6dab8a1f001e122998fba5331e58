uint32_t sensor_value;

int main(void){

	HAL_Init();
	uart_init();
//	tim_timebase_init();
	adc_single_conversion_init_start();

	uint32_t value;

    while (1)
    {
    	value = pa0_adc_read();
    	printf("%" PRIu32 ": number \n", value);
    	HAL_Delay(10);
    }
}
